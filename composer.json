{
    "name": "akamon/upsight-php-sdk-wrapper",
    "type": "library",
    "description": "This is a wrapper that uses `composer` for Upsight PHP SDK library.",
    "keywords": ["upsight", "kontagent", "sdk"],
    "homepage": "http://help.analytics.upsight.com/api-sdk-reference/php/",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordi Llonch",
            "email": "jordi.llonch@akamon.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Akamon\\Library\\Upsight": "src/"
        },
        "files": ["vendor/upsight/kontagent_api.php"]
    }
}
